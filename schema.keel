enum Gender {
    Male
    Female
    Nonbinary
    Other
}

enum Region {
    Asia
    Americas
    Africa
    Mena
    Europe
    Poles
    Oceania
}

model Book {
    fields {
        isbn8 Text
        isbn13 Text
        name Text
        author Author
        published Date
    }
}

model Author {
    fields {
        givenname Text
        surname Text
        gender Gender
        region Region
    }
}
